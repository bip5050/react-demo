{
  "name": "esm-frontend",
  "version": "0.10.1",
  "description": "The simplest Async Universal React & Redux boilerplate I could possibly make.",
  "main": "src/server/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "dev-server": "cross-env BABEL_ENV=server nodemon ./src/server/index.js --exec babel-node --ignore dist/ -e js,scss,css",
    "dev-client": "cross-env webpack --config ./webpack.config.dev.js",
    "dev": "concurrently \"cross-env npm run dev-server\" \"cross-env npm run dev-client\"",
    "prod-server": "cross-env BABEL_ENV=server babel src --out-dir ./dist",
    "prod-client": "cross-env webpack --config ./webpack.config.prod.js",
    "build": "npm run clean && npm run prod-server && npm run prod-client",
    "start:dev": "cross-env node ./dist/server/index.js",
    "start": " node ./dist/server/index.js",
    "staging-server": "cross-env BABEL_ENV=server NODE_ENV=staging babel src --out-dir ./dist",
    "staging-client": "cross-env NODE_ENV=staging webpack --config ./webpack.config.prod.js",
    "staging-build": "npm run clean && npm run staging-server && npm run staging-client",
    "staging-start": " NODE_ENV=staging node ./dist/server/index.js"
  },
  "author": "foodjets",
  "license": "MIT",
  "keywords": [
    "react",
    "isomorphic",
    "universal",
    "webpack",
    "express",
    "redux",
    "boilerplate",
